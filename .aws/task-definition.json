{
  "family": "geojson-processor",
  "networkMode": "awsvpc",
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "256",
  "memory": "512",
  "executionRoleArn": "arn:aws:iam::518394500999:role/ecs-execution-role",
  "taskRoleArn": "arn:aws:iam::518394500999:role/ecs-task-role",
  "containerDefinitions": [
    {
      "name": "geojson-processor",
      "image": "518394500999.dkr.ecr.eu-central-1.amazonaws.com/geojson-processor:latest",
      "essential": true,
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/geojson-processor",
          "awslogs-region": "eu-central-1",
          "awslogs-stream-prefix": "ecs"
        }
      },
      "environment": [
        {
          "name": "AWS_REGION",
          "value": "eu-central-1"
        },
        {
          "name": "DB_HOST",
          "value": "my-postgres-db.cly4yc2qy3sz.eu-central-1.rds.amazonaws.com"
        },
        {
          "name": "DB_PORT",
          "value": "5432"
        },
        {
          "name": "DB_NAME",
          "value": "mydb"
        },
        {
          "name": "DB_USER",
          "value": "dbadmin"
        },
        {
          "name": "DB_PASSWORD",
          "value": "Hello123"
        }
      ]
    }
  ]
}